{
    "base_url": "http://graphite.domain",
    "metrics_load_info": {
        "API response time": "",
        "System uptime": ""
    },
    "logs": {
        "version": 1,
        "disable_existing_loggers": false,
        "formatters": {
            "simple": {
                "format": "%(asctime)s - %(name)s - %(levelname)s - %(message)s"
            }
        },
        "handlers": {
            "console": {
                "class": "logging.StreamHandler",
                "level": "DEBUG",
                "formatter": "simple",
                "stream": "ext://sys.stdout"
            }
        },
        "loggers": {
            "asyncio": {
                "level": "WARNING",
                "handlers": [ "console" ]
            }
        },
        "root": {
            "level": "ERROR",
            "handlers": [ "console" ]
        }
    }
}